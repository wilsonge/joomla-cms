      "dependencies": [
        "core",
        "grapes"
      ],
      "attributes": {
        "defer": true
      }
    },
    {
      "name": "com_templates.pagebuilder",
      "type": "style",
      "uri": "com_templates/admin-template-pagebuilder.min.css",
      "dependencies": [
        "grapes"
      ]
    },
    {
      "name": "com_templates.pagebuilder",
      "type": "preset",
      "dependencies": [
        "com_templates.pagebuilder#style",
        "com_templates.pagebuilder#script"
      ]
    }
  ]
}
