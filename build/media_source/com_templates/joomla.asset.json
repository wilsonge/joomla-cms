{
  "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
  "name": "com_templates",
  "version": "4.0.0",
  "description": "Joomla CMS",
  "license": "GPL-2.0-or-later",
  "assets": [
    {
